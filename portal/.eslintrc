{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "no-empty": [
      1,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-case-declarations": 0,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  },
  "globals": {
    "dispatch": true
  },
  "extends": "eslint:recommended"
}
